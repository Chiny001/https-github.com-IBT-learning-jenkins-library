pipeline{
    agent any


stages {
        stage('1st- stage') {
            steps {
                echo 'Hello World'

            }
        }
        stage('2nd- stage | git check-out') {
            steps {
                echo 'git will be checking out here'
                checkout changelog: false, poll: false, scm: [$class: 'GitSCM', branches: [[name: '*/hansn-feaure']], extensions: [], userRemoteConfigs: [[credentialsId: 'ibt', url: 'https://github.com/IBT-learning/ibt-maven.git']]]

            }
        }

        stage('3rd- stage') {
                    steps {
                        echo 'Hello World'

                    }
                }
    }
}