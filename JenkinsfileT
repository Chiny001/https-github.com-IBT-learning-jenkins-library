pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi this tojja'
            }
        }
        stage('trying jenkinsfile') {
              steps {
                   echo 'this is running from me'
             }
        }
        stage('git checkout'){
        steps{
        git branch: 'feature_tojja', changelog: false, credentialsId: 'git_credentials_tojja', poll: false, url: 'https://github.com/IBT-learning/DevOps-Git.git'

        }
       }
    }
}