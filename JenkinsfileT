pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi this tojja'
            }
        }
        stage('trying jenkinsfile') {
              steps {
                   echo 'this is running from me'
             }
        }
    }
}