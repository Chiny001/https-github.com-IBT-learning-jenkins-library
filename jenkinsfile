pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('hi') {
             steps{
                 echo 'hi'
      }
     }
        stage('howdy') {
    	     steps{
    		      echo 'howdy'
    	}
      }
      steps{
            git branch: 'main', changelog: false, credentialsId: 'git_credential', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
        }
   }
   stage('list files') {
    steps
       'dir'
    }
   }