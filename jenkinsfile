pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'HI'
            }
        }
          stage('test') {
            steps {
                echo 'test'
            }
        }
        stage('git chechout') {
         steps{
         checkout scmGit(branches: [[name: '*/feature/yannick']], extensions: [], userRemoteConfigs: [[credentialsId: 'yannick-jenkins', url: 'https://github.com/IBT-learning/ibt-maven.git']])
         }
      }
    }
}