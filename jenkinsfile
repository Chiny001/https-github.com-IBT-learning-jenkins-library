pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'HI'
            }
        }
          stage('Hi people') {
            steps {
                echo 'My name is Chia'
            }
        }
        stage('Git checkout'){
            steps{
                checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/feature-Chia']], extensions: [], userRemoteConfigs: [[credentialsId: 'git-credentials', url: 'https://github.com/IBT-learning/ibt-maven.git']])
            }
        }
    }
}
