pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Testing jenkinsfile') {
                    steps {
                        echo 'jenkinsfile'
                    }
                }
        stage('Good-bye') {
            steps {
                echo 'Good-bye'
            }
        }
        stage('Git checkout') {
            steps{
                checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/feature-irinamm']], extensions: [], userRemoteConfigs: [[credentialsId: 'ibt', url: 'https://github.com/IBT-learning/ibt-maven.git']])
                sh 'ls -lrt'
            }
        }
    }
}
