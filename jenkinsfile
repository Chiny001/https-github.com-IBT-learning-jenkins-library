pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Hi') {
            steps {
                echo 'Hi'
            }
        }
         stage('Git checkout') {
            steps {
                echo 'Hi this is my first pipeline job'
                git branch: 'feature-fabrice', changelog: false, credentialsId: 'for-github', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
         stage('Gitscm') {
            steps {
                 echo 'trying gitscm to hooks logs'
                 echo 'trying gitscm poll'
            }
         }
    }
}