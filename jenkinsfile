pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
       stage('Hi') {
            steps {
                echo 'Hi'
            }
       }
       stage('Git Checkout'){
            steps {
                checkout scmGit(branches: [[name: '*/feature/james']], extensions: [], userRemoteConfigs: [[credentialsId: 'git_James_c', url: 'https://github.com/IBT-learning/ibt-maven.git']])
            }
       }
       stage('List files'){
            steps {
                sh 'ls'
            }
       }
       stage('testing webhook'){
            steps {
                echo "success webhook"
            }
       }
    }
}
