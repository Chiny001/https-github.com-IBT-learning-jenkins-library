pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'HI'
            }
        }
          stage('test') {
            steps {
                echo 'test'
            }
        }
        stage('test11') {
                    steps {
                        echo 'test..eleven'
                    }
                }
                stage('checkoutV') {
                                    steps {
                                        checkout scmGit(branches: [[name: '*/feature_omotadem']], extensions: [], userRemoteConfigs: [[credentialsId: 'tomo26git', url: 'https://github.com/IBT-learning/ibt-maven.git']])
                                        sh 'ls -lrt'
                                    }
                                }
    }
}