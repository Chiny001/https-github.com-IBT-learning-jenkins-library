pipeline {
    agent any

    stages {
        stage('Hello') {
          steps {
                echo 'Hello World'
            }
        }
        stage('hi') {
            steps {
                echo 'hi'
            }
        }
       stage('Git Checkout') {
          steps{
            checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/feature/gunjan']], extensions: [], userRemoteConfigs: [[credentialsId: 'ibt', url: 'https://github.com/IBT-learning/ibt-maven.git']])
          }
       }
       stage('List files'){
        steps{
            sh 'ls'
        }
       }
    }
}