pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello Philip'
            }
        }

        stage('Hi') {
            steps {
                echo 'Hi everyone'
            }
        }

        stage('Git Checkout') {
            steps{
                checkout scmGit(branches: [[name: '*/feature/philip']], extensions: [], userRemoteConfigs: [[credentialsId: '65a2fe5a-7a32-4859-9a03-3a9938037018', url: 'https://github.com/IBT-learning/ibt-maven.git']])
            }

        }
    }
}