pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi'
            }
        }
        stage('test') {
            steps {
                echo 'test'
            }
        }
        stage('Git checkout') {
                    steps {
                    checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/feature_claudia']], extensions: [], userRemoteConfigs: [[credentialsId: 'IBT-github-claudia', url: 'https://github.com/IBT-learning/ibt-maven.git']])}                    sh 'ls -lrt'
            }
        }

