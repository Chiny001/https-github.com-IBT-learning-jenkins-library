pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Hi') {
            steps {
                echo 'Hello nad'
            }
        }
         stage('Git checkout') {
            steps {
                echo 'adding git'
                git branch: 'hansn-feature', changelog: false, credentialsId: 'ibt', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage('GitSCM') {
                    steps {
                        echo 'test gitscm'

                    }
                }
         stage('MVN clean') {
                            steps {
                                echo 'test gitscm'
                                mvn clean
                            }
                        }
    }
}
