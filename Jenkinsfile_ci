pipeline {
   agent any
    tools {
            maven 'maven_3.9.3'
        }


      stages{
      stage('git checkout'){
         steps{
           git branch: 'feature_abem', changelog: false, credentialsId: 'git_crefentials_abe', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
         }
      }
      stage('Validate'){
         steps{
            withMaven(maven: 'maven_3.9.3') {
                sh 'mvn validate'
            }
         }
      }
      stage('Compile'){
        steps{
           sh 'mvn -version'
           sh 'mvn compile'
        }
      }
      }
}