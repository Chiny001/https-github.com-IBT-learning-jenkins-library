pipeline{
agent any
tools { maven 'maven_3.8' }
stages{
    stage('git checkout'){
        steps{
            checkout scmGit(branches: [[name: '*/feature-dianna']], extensions: [], userRemoteConfigs: [[credentialsId: 'ibt', url: 'https://github.com/IBT-learning/ibt-maven.git']])
      }
     }
     stage('Compile'){
        steps{
           bat 'mvn compile'
        }
     }
   }
}

