pipeline {
    agent any

    tools {
        // Use the correct name of the installed Maven tool
        maven 'maven_3.8.3'
    }

    stages {
        stage('git checkout') {
            steps {
                checkout(scm: [$class: 'GitSCM',
                              branches: [[name: '*/feature-dianna']],
                              userRemoteConfigs: [[credentialsId: 'ibt',
                                                  url: 'https://github.com/IBT-learning/ibt-maven.git']]])
            }
        }
        stage('Compile') {
            steps {
                bat 'mvn compile'
            }
        }
    }
}

