pipeline{
        agent any

      stages{
      stage('Git checkout'){
        steps{
          git branch: 'feature_tojja', changelog: false, credentialsId: 'git_credentials_tojja', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
        }
      }
      stage('validate'){
           steps{
               withMaven( maven: 'maven_3.9.3') {
                   sh 'mvn validate'
               }
           }
      }
      }
}