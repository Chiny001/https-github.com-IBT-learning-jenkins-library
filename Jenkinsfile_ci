pipeline {
agent any
tool name: 'maven_3.8', type: 'maven'
stages {
    stage ('git checkout') {
        steps {
            checkout scmGit(branches: [[name: '*/feature-laura']], extensions: [], userRemoteConfigs: [[credentialsId: 'git_credential_laura', url: 'https://github.com/IBT-learning/ibt-maven.git']])
        }
    }
    stage ("validate") {
        steps {
            sh "mvn validate"
        }
    }
}
}