pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello Github'
                git branch: 'feature_koby2', changelog: false, credentialsId: 'for-github', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage('validate') {
            steps {
                echo 'Hi Mvn'
                withMaven(maven: 'maven_3.8') {
                sh "mvn validate"
                }
            }
        }
    }
}
