pipeline {
    agent any

    stages{
    stage ('Git checkout') {
        steps{
            git branch: 'feature-franck' , credentialsId: 'GtiHub_user_cred_franck', url: 'https://github.com/IBT-learning/ibt-maven.git'
        }
    }
    stage('Validate'){
        steps {
            withMaven( maven: 'maven_3.8') {
                mvn validate
            }
        }
      }
  }
