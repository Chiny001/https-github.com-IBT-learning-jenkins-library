pipeline {
agent any
tools { name: 'maven_3.8', type: 'maven' }
stages {
    stage('Git checkout'){
     steps{
        checkout scmGit(branches: [[name: '*/feature-gunj']], extensions: [], userRemoteConfigs: [[credentialsId: 'ibt', url: 'https://github.com/IBT-learning/ibt-maven.git']])
     }
    }
    stage('Validate'){
        steps{
            sh 'mvn validate'
        }
    }
     stage('Compile'){
            steps{
                sh 'mvn compile'
            }
        }
   }
}