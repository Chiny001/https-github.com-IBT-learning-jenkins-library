pipeline {
    agent any

    stages{
    stage('Git checkout') {
        steps{
            https://github.com/IBT-learning/ibt-maven.git
        }
}
    stage ('Validate') {
        steps{
            withMaven(maven: 'maven_3.8') {
                 mvn validate
            }
        }
    }
     stage ('Compile') {
        steps{
            withMaven(maven: 'maven_3.8') {
                 mvn compile
        }
        }

      }
 }
}