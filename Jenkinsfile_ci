pipeline {
    agent any

    stages {
        stage('Git checkout') {
            steps {
             checkout scmGit(branches: [[name: '*/official/josias']], extensions: [], userRemoteConfigs: [[credentialsId: 'jenkins-josias', url: 'https://github.com/IBT-learning/ibt-maven.git']])
           }
        }
    }
}