pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi'
            }
        }
         stage('Hi test') {
            steps {
                echo 'Hi testing Jenkins'
            }
        }
        stage('Jenkinsfile test') {
                    steps {
                        echo 'Jenkinsfile test'
                    }
                }
    }
}
