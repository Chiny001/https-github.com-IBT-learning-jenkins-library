pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hello this is abraham'
            }
        }
        stage('Hiiii') {
                    steps {
                        echo 'this is running from github'
                    }
                }
    }
}