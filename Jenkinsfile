pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello, World!'
            }
        }
        stage('Hi') {
            steps {
                echo 'hi!'
            }
        }
        stage('my name is dianna, what about you ') {
            steps {
                echo 'my name is dianna what about you '
            }
        }
    }
}