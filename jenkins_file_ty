pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi'
            }
        }
    stage('How are you') {
                steps {
                    echo 'Hoe are you'
                }
            }
            stage{'git checkout') {
                steps{
                    git branch: 'feature_otunba', credentialsId: 'git_password', url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
       }
       stage('list files') {
         steps{
              bat 'dir'
              sh 'ls -lrt'
    }
   }
    }
  }