pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello '
            }
        }

                    stage('Hello world test') {
                          steps {
                             echo 'Hello world  test'
                           }
                    }

                              stage("Hello World second script"){

                                           steps{
                                               echo "Hello World second script"
                                            }
                              }

                                 stage{
                                   stage('git checkout'){
                                      steps{
                                         git branch: 'feature-rizme' , changelog: false, credentialsId: 'GitHub_user_cred_gunjan', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
                                      }
                                   }
                               }
     }

}