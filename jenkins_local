pipeline {
    agent any

    stages{
      stage('Git checkout') {
          steps{

             checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/feature-rizme']], extensions: [], userRemoteConfigs: [[credentialsId: 'riz_jenken', url: 'https://github.com/IBT-learning/ibt-maven.git']])
          }
      }
               tage('Validate'){
                       steps{
                           withMaven( maven: 'maven_3.8', mavenSettingsConfig: '', traceability: true) {
                               // some block
                            }
                       }
               }
                   tage('Validate'){
                           steps{
                               withMaven(maven: 'maven_3.8') {
                                  sh 'mvn validate'
                               }
                           }
                    }

    }


}