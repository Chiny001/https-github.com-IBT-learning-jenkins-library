pipeline {
    agent any

    stages {
        stage('1st- great') {
            steps {
                echo 'Hello World'
            }
        }
         stage('2nd- prompt') {
            steps {
                echo 'Hi'
            }
        }
        stage('adding Git') {
            steps {
                echo 'first pipeline job'
                git branch: 'hansn-feature', credentialsId: 'for-github', url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage('Gutscm') {
                    steps {
                        echo 'trying git scm'
                    }
                }
                stage('adding sonarqube') {
                            steps {
                                echo 'Hi'
                            }
                        }
    }
}
