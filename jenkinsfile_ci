pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello '
            }
        }

                    stage('Hello world test') {
                          steps {
                             echo 'Hello world  test'
                           }
                    }

                              stage("Hello World second script"){

                                           steps{
                                               echo "Hello World second script"
                                            }
                              }

     }

}

