pipeline {
    agent any

    stages {
        stage('Hi') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hello') {
            steps {
                echo 'Hi this is chisom'
            }
        }
        stage('trying jenkinsfile1'){
            steps{
               echo "this is running from github"
            }
        }
        stage('Git checkout'){
           steps{
              git branch: 'feature_chisom', changelog: false, credentialsId: 'ibt', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage('list files'){
            steps{
                sh 'ls -lrt'
               }
            }
        }
    }