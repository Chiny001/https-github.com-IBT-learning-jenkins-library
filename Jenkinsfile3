pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/feature_koby2']], extensions: [], userRemoteConfigs: [[credentialsId: 'for-github', url: 'https://github.com/IBT-learning/ibt-maven.git']])
            }
        }
        stage('Git') {
            steps {
                sh "Mvn validate"
            }
        }
    }
}
