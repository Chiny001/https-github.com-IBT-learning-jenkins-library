pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
     stage('Hi') {
          steps {
                    echo 'Hi my name is victor'
      }
      }
      stage('git checkout') {
            steps{
                git changelog: false, credentialsId: 'git_credential', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
         }
    }
    stage('list files') {
      steps{
               bat 'dir'
     }
   }
     }
}