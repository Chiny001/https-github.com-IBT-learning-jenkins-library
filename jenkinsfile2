pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi me'
            }
        }
        stage('Test') {
            steps {
                echo 'TEXT Me Omotade'
            }
        }
        stage('Testing jenkins') {
                    steps {
                        echo 'TEXT Me Omotade, testing jenkins file'
                    }
                }
    }
}
