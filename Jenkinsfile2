pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hey') {
            steps {
                echo 'Hey this is Ros!'
            }
        }
        stage('Trying Jenkinsfile'){
            steps{
                echo "this is running from github"
            }
        }
        stage('Git Checkout'){
            steps{
                git branch: 'feature_ros', changelog: false, credentialsId: 'git_creds', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage('Listing files') {
             steps {
                sh 'ls -lrt'
             }
        }
    }
}