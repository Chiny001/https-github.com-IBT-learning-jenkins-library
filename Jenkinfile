pipeline {
     agent any
     parameters{
     string(name:'Branch_Name'. defaultvalue:'main'. description:'Enter the branch to checkout'
     choice(name: 'CHOICES', choices: ['one', 'two', 'three'], description: '') }
     }
        stages {
            stage('Hello') {
                steps {
                    echo 'Hello World'
                }
            }
        stage('Hi') {
            steps {
                    echo 'Hi'
                }
            }
        stage('Test') {
            steps {
                    echo 'Test'
                }
            }
        stage('Testing Jenkinfile') {
            steps {
                echo 'Jenkinfile'
                }
            }
        stage("Git checkout") {
            steps{
                checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/feature_Taiwo']], extensions: [], userRemoteConfigs: [[credentialsId: 'ibt-github', url: 'https://github.com/IBT-learning/ibt-maven.git']])
                sh "ls -lrt"
            }
        }
    }
}




