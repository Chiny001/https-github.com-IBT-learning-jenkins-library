pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
          stage('Hi') {
            steps {
                echo 'Hi'
            }
        }
         stage('Test') {
            steps {
                echo 'Test'
            }
        }
        stage('Testing Jenkinfile') {
                    steps {
                        echo 'Jenkinfile'
                    }
                }
    }
}
